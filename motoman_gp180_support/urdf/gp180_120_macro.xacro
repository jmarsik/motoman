<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_gp180_120" params="prefix">

    <!-- Properties -->
    <material name="yaskawa_blue">
          <color rgba="0.12941 0.14902 0.74902 1" />
    </material>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <origin rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_1_s">
        <visual>
            <origin xyz="0 0 -0.317" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/link_1_s.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 -0.317" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/link_1_s.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_2_l">
        <visual>
            <origin xyz="0.325 0.07 -0.650" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/link_2_l.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0.325 0.07 -0.650" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/link_2_l.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_3_u">
        <visual>
            <origin xyz="0.325 0.0415 -1.800" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/link_3_u.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0.325 0.0415 -1.800" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/link_3_u.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_4_r">
        <visual>
            <origin xyz="1.723 0 -2.100" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/link_4_r.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="1.723 0 -2.100" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/link_4_r.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_5_b">
        <visual>
            <origin xyz="1.915 -0.082 -2.100" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/link_5_b.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="1.915 -0.082 -2.100" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/link_5_b.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_6_t">
        <visual>
            <origin xyz="2.1145 0 -2.100" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/visual/link_6_t.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="2.1145 0 -2.100" rpy="${radians(90)} 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp180_support/meshes/gp180_120/collision/link_6_t.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <!-- corresponds to Yaskawa controller flange coordinate system; Z+ out from the flange, X+ up when robot is in basic position -->
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_1_s"/>
        <origin xyz="0 0 0.317" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${radians(-180)}" upper="${radians(180)}" effort="1300" velocity="${radians(125)}"/>
    </joint>
    <joint name="${prefix}joint_2_l" type="revolute">
        <parent link="${prefix}link_1_s"/>
        <child link="${prefix}link_2_l"/>
        <origin xyz="-0.325 -0.07 0.333" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-60)}" upper="${radians(76)}" effort="1100" velocity="${radians(115)}"/>
    </joint>
    <joint name="${prefix}joint_3_u" type="revolute">
        <parent link="${prefix}link_2_l"/>
        <child link="${prefix}link_3_u"/>
        <origin xyz="0 0.0285 1.150" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-86)}" upper="${radians(90)}" effort="900" velocity="${radians(125)}"/>
    </joint>
    <joint name="${prefix}joint_4_r" type="revolute">
        <parent link="${prefix}link_3_u"/>
        <child link="${prefix}link_4_r"/>
        <origin xyz="-1.398 0.0415 0.300" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-360)}" upper="${radians(360)}" effort="883" velocity="${radians(182)}"/>
    </joint>
    <joint name="${prefix}joint_5_b" type="revolute">
        <parent link="${prefix}link_4_r"/>
        <child link="${prefix}link_5_b"/>
        <origin xyz="-0.192 0.082 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-130)}" upper="${radians(130)}" effort="883" velocity="${radians(175)}"/>
    </joint>
    <joint name="${prefix}joint_6_t" type="revolute">
        <parent link="${prefix}link_5_b"/>
        <child link="${prefix}link_6_t"/>
        <origin xyz="-0.1995 -0.082 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-360)}" upper="${radians(360)}" effort="520" velocity="${radians(265)}"/>
    </joint>
    <joint name="${prefix}joint_6_t-tool0" type="fixed">
        <origin xyz="-0.0255 0 0" rpy="0 ${radians(-90)} 0"/>
        <parent link="${prefix}link_6_t"/>
        <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <!-- corresponds to Yaskawa controller robot coordinate system -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.317" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

</xacro:macro>
</robot>
